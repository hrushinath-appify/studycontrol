{
  "version": 2,
  "builds": [
    {
      "src": "client/package.json",
      "use": "@vercel/next"
    },
    {
      "src": "api/health.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/backend.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/debug-env.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/test-email.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/manual-verify.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/v1/(.*)",
      "dest": "api/backend.js"
    },
    {
      "src": "/health",
      "dest": "api/health.js"
    },
    {
      "src": "/api/debug",
      "dest": "api/health.js"
    },
    {
      "src": "/debug-env",
      "dest": "api/debug-env.js"
    },
    {
      "src": "/test-email",
      "dest": "api/test-email.js"
    },
    {
      "src": "/manual-verify",
      "dest": "api/manual-verify.js"
    },
    {
      "src": "/(.*)",
      "dest": "client/$1"
    }
  ]
}