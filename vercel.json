{
  "version": 2,
  "builds": [
    {
      "src": "client/package.json",
      "use": "@vercel/next"
    },
    {
      "src": "api/health.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/backend.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/v1/(.*)",
      "dest": "api/backend.js"
    },
    {
      "src": "/health",
      "dest": "api/health.js"
    },
    {
      "src": "/api/debug",
      "dest": "api/health.js"
    },
    {
      "src": "/(.*)",
      "dest": "client/$1"
    }
  ]
}